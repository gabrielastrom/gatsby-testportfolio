{
  "siteMetadata": {
    "language": "en",
    "siteUrl": "https://gatsby-starter-portfolio-minimal-theme.netlify.app/",
    "title": "Gabriel's Portfolio",
    "titleTemplate": "%s · Portfolio Minimal",
    "description": "A portfolio displaying recent projects, experience etc. Enjoy.",
    "author": "Gabriel Åström",
    "avatar": "../content/images/gatsby.png",
    "bio": "This biography will be used in the footer area of an article and can contain some more details about the author.",
  },
  "siteConfiguration": {
    "logo": { "text": "Gabriel's Portfolio" },
    "navigation": {
      "header": [
        { "label": "About", "url": "/#about" },
        { "label": "My Projects", "url": "/blog" },
        { "label": "Features", "url": "/#features" },
        { "label": "Github", "url": "/#github" }
      ],
      "ctaButton": {
        "openNewTab": true,
        "label": "Resume (CV)",
        "url": "/resume.pdf"
      },
      "footer": [
        { "label": "About", "url": "/#about" },
        { "label": "Linkedin", "url": "/#linkedin" }
      ]
    },
    "featureToggles": {
      "useDarkModeAsDefault": false,
      "useDarkModeBasedOnUsersPreference": true,
      "useCookieBar": false
    }
  }
}
